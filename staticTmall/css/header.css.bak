#header{
	width:100%;
	height:32px;
	background-color:rgb(242,242,242);
	border-bottom:1px solid rgb(229,229,229);
}
#header .h-list{
	width:1320px;
	margin:0 auto;
	font-size:12px;
	color:rgb(153,153,153);
	line-height:32px;
}
#header .h-list a{
	color:rgb(153,153,153);
}
#header .red a:hover{
	color:red;
}
#header .h-list li{
	float:left;
}
#header .h-list .h-list-left li{
	margin-right:20px;
}
#header .h-list .h-list-right{
	position:relative;
	float:right;
}
#header .h-list .h-list-right li{
	margin:0 10px 0 10px; 
}
#header i{
	font-size:12px;
	color:red;
}
.underline a:hover{
	text-decoration:underline;
}
#header .h-list-right .my-taobao{
	display:none;
	position:absolute;
	padding:0 4px;
	left:9px;
	top:32px;
	line-height:19px;
	border:1px solid rgb(229,229,229);
	border-top:0px solid rgb(229,229,229);
}
#header .h-list-right p{
	margin:5px;
}
#header .h-list-right .h-width{
	padding:10px;
}
#header .h-list-right .background-white{
	background-color:rgb(242,242,242);
}
#header .h-list-right .background-white:hover{
	background-color:white;
}
#header .h-list-right b{
	position:relative;
	top:-2px;
	right:5px;
	display:inline-block;
	border-left:3px solid rgba(0,0,0,0);
	border-right:3px solid rgba(0,0,0,0);
	border-top:3px solid rgb(153,153,153);
}
